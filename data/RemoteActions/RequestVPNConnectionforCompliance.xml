    <Selector></Selector>
    <Investigation>
      <Label></Label>
      <Type>source</Type>
      <Description />
    </Investigation>
    <InvestigationRaw>
        </InvestigationRaw>
  </Target>
  <ScriptInfo RunAs="localSystem" Timeout="720">
      <WindowsScript Hash="213be970754f6e6bb93437c09d73311a822fe18f030651dd6f1fc384d59fcf1a"></WindowsScript>
        <Inputs>
                    <Input Name="EnforcePolicyUpdate" Value="&quot;&quot;" UsedBy="WindowsScript" />
                              <Input Name="CampaignId" Value="d7f50034-9401-4448-a588-aaa873d71850" UsedBy="WindowsScript" />
                              <Input Name="CorporateDnsDomains" Value="&quot;&quot;" UsedBy="WindowsScript" />
                              <Input Name="DaysSinceLastGPOUpdate" Value="2" UsedBy="WindowsScript" />
                </Inputs>
    <Outputs>
