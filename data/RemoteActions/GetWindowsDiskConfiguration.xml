          <Description>Interface used by the physical disk where the drive is located</Description>
      </Output>
                <Output UID="9a60a214-00c5-9976-7cbd-bc3a2af8fb75" Name="DriveIndexingEnabled" Type="uint32" Label="Drive Indexing Enabled" Format="ps_boolean" Object="source" UsedBy="WindowsScript">
