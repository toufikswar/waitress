        <Inputs>
    </Inputs>
    <Outputs>
            <Output UID="8126b6f1-2e64-7951-b11e-a52eac629c0a" Name="Battery1Health" Type="real" Label="Battery1 Health" Format="ratio_small" Object="source" UsedBy="WindowsScript">
          <Description>Health ratio for battery 1, expressed as a percentage</Description>
      </Output>
                <Output UID="e7700962-e322-eaaa-f630-052d2eab5f97" Name="Battery1DesignedCapacity" Type="uint32" Label="Battery1 Designed Capacity" Format="integer" Object="source" UsedBy="WindowsScript">
