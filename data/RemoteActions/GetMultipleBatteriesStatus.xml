      </Output>
                <Output UID="3ac1f563-1d2f-ad00-3560-c688e58f37a4" Name="EstimatedBatteryLife" Type="uint32" Label="Estimated Battery Life" Format="duration_ms" Object="source" UsedBy="WindowsScript">
