<?xml version="1.0" encoding="UTF-8"?>
<Action UID="7141ff1b-fbb1-1a80-2d9c-f9d31683377e" SyntaxVersion="10" DataModelVersion="13" Name="Get Network Speed" AutomaticScheduling="disabled" Description="Gets information about the speed of the network by measuring the Web RTT against an external URL and a business URL. It also checks whether the connection is metered or not.">
  <Target>
    <Selector></Selector>
    <Investigation>
      <Label></Label>
      <Type>source</Type>
      <Description />
    </Investigation>
    <InvestigationRaw>
        </InvestigationRaw>
  </Target>
  <ScriptInfo RunAs="interactiveUser" Timeout="720">
      <WindowsScript Hash="8c3abbc24d7cf8e6766a913ede4d13cf574e8062201827b93ac2ecbb8577c5d7"></WindowsScript>
        <Inputs>
                    <Input Name="MaximumDelayInSeconds" Value="30" UsedBy="WindowsScript" />
                              <Input Name="ExternalURL" Value="www.google.com" UsedBy="WindowsScript" />
                              <Input Name="WebRTTThreshold" Value="600" UsedBy="WindowsScript" />
                              <Input Name="BusinessURL" Value="www.google.com" UsedBy="WindowsScript" />
                              <Input Name="BusinessWebRTTThreshold" Value="600" UsedBy="WindowsScript" />
                </Inputs>
    <Outputs>
            <Output UID="430e6cd8-90e6-8297-55bd-eb29e1a6e06e" Name="LastWebRTTToExternalURL" Type="uint32" Label="Last Web RTT to external URL" Format="duration_ms" Object="source" UsedBy="WindowsScript">
          <Description>URL Web RTT of the last execution</Description>
      </Output>
                <Output UID="282eb100-8ecb-46e7-29eb-db7c3c1a55d4" Name="AverageWebRTTToExternalURL" Type="uint32" Label="Average Web RTT to external URL" Format="duration_ms" Object="source" UsedBy="WindowsScript">
          <Description>The average external URL Web RTT measured between all executions within the current day</Description>
      </Output>
                <Output UID="5ba2a0e2-ee4b-dbb4-90e3-7679f1109b47" Name="TimesExternalURLAboveWebRTTThreshold" Type="uint32" Label="Times external URL above Web RTT threshold" Format="integer" Object="source" UsedBy="WindowsScript">
          <Description>All the times that the external URL Web RTT has been above the threshold within the current day</Description>
      </Output>
                <Output UID="d9ff7b5d-0ed1-6030-7441-fe9d099c4419" Name="LastWebRTTToBusinessURL" Type="uint32" Label="Last Web RTT to business URL" Format="duration_ms" Object="source" UsedBy="WindowsScript">
          <Description>Business URL Web RTT of the last execution</Description>
      </Output>
                <Output UID="75a257ab-bb1f-0730-6a05-163080f580b6" Name="AverageWebRTTToBusinessURL" Type="uint32" Label="Average Web RTT to business URL" Format="duration_ms" Object="source" UsedBy="WindowsScript">
          <Description>The average business URL Web RTT measured between all executions within the current day</Description>
      </Output>
                <Output UID="aab346ad-9268-5ab0-c0f9-7f53b705a4e8" Name="TimesBusinessURLAboveWebRTTThreshold" Type="uint32" Label="Times business URL above Web RTT threshold" Format="integer" Object="source" UsedBy="WindowsScript">
          <Description>All the times that the business URL Web RTT has been above the threshold within the current day</Description>
      </Output>
                <Output UID="661a5fb9-ca98-6445-3d25-7536e58fff0f" Name="TypeOfConnection" Type="string" Label="Type Of Connection" Format="string" Object="source" UsedBy="WindowsScript">
          <Description>Type of physical connection</Description>
      </Output>
                <Output UID="f62af0eb-2e52-29f1-92ee-b680b11cd72f" Name="MeteredConnection" Type="uint32" Label="Metered Connection" Format="ps_boolean" Object="source" UsedBy="WindowsScript">
          <Description>Whether the connection is metered or unrestricted</Description>
      </Output>
          </Outputs>
