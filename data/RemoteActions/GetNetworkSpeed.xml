  <Schedule>
    <EvaluationFrequency>60</EvaluationFrequency>
    <ExecutionFrequency>60</ExecutionFrequency>
  </Schedule>
  <ManualTrigger Type="all" Status="enabled" IsMultiDevicesTriggerable="true">
    <Selector>select source</Selector>
  </ManualTrigger>
  <Purpose>1</Purpose>
  </Action>