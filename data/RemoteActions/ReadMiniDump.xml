        <Inputs>
                    <Input Name="BlueScreenViewPath" Value="C:\Program Files (x86)\NirSoft\BlueScreenView\BlueScreenView.exe" UsedBy="WindowsScript" />
                </Inputs>
    <Outputs>
            <Output UID="43617a27-e08c-cdfb-8526-b2956901dfbe" Name="MiniDumpDetected" Type="uint32" Label="Mini Dump Detected" Format="ps_boolean" Object="source" UsedBy="WindowsScript">
