<Action UID="58928659-985c-7903-2748-761a562b4e82" SyntaxVersion="10" DataModelVersion="13" Name="Get MS Office information" AutomaticScheduling="enabled" Description="Returns several Microsoft Office-related information, such as the version of the suite, the applications installed, the language preferences, the size of the user's Outlook Data Files and the used space in the OneDrive business account folder.
Useful for troubleshooting both Office-related and disk space issues and for managing the Office migration to the cloud. The script is developed for Windows 10 and 7 and Microsoft Office 2010 onwards.">
  <Target>
    <Selector></Selector>
    <Investigation>
      <Label></Label>
      <Type>source</Type>
      <Description />
    </Investigation>
    <InvestigationRaw>
        </InvestigationRaw>
  </Target>
  <ScriptInfo RunAs="interactiveUser" Timeout="120">
      <WindowsScript Hash="90c48d7447366232b8105082417aec8f009cf5b254d97e7aa9d0553a137d531a"></WindowsScript>
        <Inputs>
    </Inputs>
    <Outputs>
            <Output UID="a6f2427b-9cf4-556a-a338-c86521528b06" Name="MSOfficeVersion" Type="list_of_strings" Label="MS Office Version" Format="string_array" Object="source" UsedBy="WindowsScript">
          <Description>Version of the Microsoft Office suite installed on the device</Description>
      </Output>
                <Output UID="58f59ef4-53c9-bdaf-5f0e-16fcef8dba35" Name="InstalledApplications" Type="list_of_strings" Label="Installed Applications" Format="string_array" Object="source" UsedBy="WindowsScript">
          <Description>List of installed Office applications</Description>
      </Output>
                <Output UID="720da662-c47b-f2ba-5182-6e572034b362" Name="ActiveDisplayLanguage" Type="list_of_strings" Label="Active Display Language" Format="string_array" Object="source" UsedBy="WindowsScript">
          <Description>Display language currently in use</Description>
      </Output>
                <Output UID="d12dc038-ee2b-3efe-5c8c-f4d22edb7cfd" Name="InstalledDisplayLanguages" Type="list_of_strings" Label="Installed Display Languages" Format="string_array" Object="source" UsedBy="WindowsScript">
          <Description>List of display languages installed on the device</Description>
      </Output>
                <Output UID="7ba7c1bc-668a-b13d-5150-7af61cc555ba" Name="InstalledEditingLanguages" Type="list_of_strings" Label="Installed Editing Languages" Format="string_array" Object="source" UsedBy="WindowsScript">
          <Description>List of editing languages installed on the device</Description>
      </Output>
                <Output UID="8e1b4c2f-7de9-f706-69b1-38865bc7a027" Name="NumberOfPSTfiles" Type="uint32" Label="Number Of PST Files" Format="integer" Object="source" UsedBy="WindowsScript">
