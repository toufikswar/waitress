<Action UID="ae9657d7-81dd-58bd-df4b-4dabb242e941" SyntaxVersion="10" DataModelVersion="13" Name="Get Microsoft Office Information" AutomaticScheduling="disabled" Description="Allows to detect issues linked to installed MS office productivity applications by returning the version, the installed application list, and the update channel used.
Verifies Microsoft Office installation compliance by checking if it meets a minimum version provided as input parameter.">
  <Target>
    <Selector></Selector>
    <Investigation>
      <Label></Label>
      <Type>source</Type>
      <Description />
    </Investigation>
    <InvestigationRaw>
        </InvestigationRaw>
  </Target>
  <ScriptInfo RunAs="interactiveUser" Timeout="120">
      <WindowsScript Hash="156a4a18c0f9332e3ca2092586f49020f29c4ce146bcbba4c6fd74b5879de6cd"></WindowsScript>
        <Inputs>
                    <Input Name="MinimumVersion" Value="&quot;&quot;" UsedBy="WindowsScript" />
                </Inputs>
    <Outputs>
            <Output UID="a01d31b9-3867-4708-6f0b-ec7c53a23ec2" Name="MSOfficeVersion" Type="list_of_strings" Label="MS Office Version" Format="string_array" Object="source" UsedBy="WindowsScript">
          <Description>Version number of the Microsoft Office suite installed on the device</Description>
      </Output>
                <Output UID="ad3c773f-6146-fdb0-035c-527128ffc8c2" Name="MSOfficeBuild" Type="list_of_strings" Label="MS Office Build" Format="string_array" Object="source" UsedBy="WindowsScript">
          <Description>Build number of the Microsoft Office suite installed on the device</Description>
      </Output>
                <Output UID="3eeca655-3635-0ca1-4737-c8888205fe23" Name="MSOfficeVersionName" Type="list_of_strings" Label="MS Office Version Name" Format="string_array" Object="source" UsedBy="WindowsScript">
          <Description>Version name of the Microsoft Office suite installed on the device</Description>
      </Output>
                <Output UID="22d63ff4-f35f-8eb7-d669-78ca260d8ee2" Name="InstalledApplications" Type="list_of_strings" Label="Installed Applications" Format="string_array" Object="source" UsedBy="WindowsScript">
          <Description>List of installed Office applications</Description>
      </Output>
                <Output UID="7f859d2b-ee95-b585-06ea-6338b0a3b62a" Name="UpdateChannel" Type="list_of_strings" Label="Update Channel" Format="string_array" Object="source" UsedBy="WindowsScript">
