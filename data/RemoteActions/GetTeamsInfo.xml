<?xml version="1.0" encoding="UTF-8"?>
<Action UID="fd6b8f5b-616c-de30-5c7c-a3abd5a15db5" SyntaxVersion="10" DataModelVersion="13" Name="Get Teams Info" AutomaticScheduling="enabled" Description="Shows the login errors of the last 24h, the deployment ring, network errors and calls made using Teams.">
  <Target>
    <Selector>select source.uid, source.name where product.name = "Microsoft Teams" AND source.platform = windows</Selector>
    <Investigation>
      <Label>Windows devices with Teams</Label>
      <Type>source</Type>
      <Description />
    </Investigation>
    <InvestigationRaw>
          &lt;Investigation xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" DataModelVersion="13" SyntaxVersion="3"&gt;
&lt;Label&gt;Windows devices with Teams&lt;/Label&gt;
&lt;Object&gt;source&lt;/Object&gt;
&lt;Platforms&gt;
    &lt;Platform&gt;windows&lt;/Platform&gt;
&lt;/Platforms&gt;
&lt;Description /&gt;
&lt;FieldList&gt;
&lt;string&gt;name&lt;/string&gt;
&lt;string&gt;platform&lt;/string&gt;
&lt;string&gt;entity&lt;/string&gt;
&lt;string&gt;last_ip_address&lt;/string&gt;
&lt;string&gt;computer_type&lt;/string&gt;
&lt;string&gt;os_name&lt;/string&gt;
&lt;string&gt;last_time&lt;/string&gt;
&lt;string&gt;model&lt;/string&gt;
&lt;/FieldList&gt;
&lt;CategoryList /&gt;
&lt;AggregateList /&gt;
&lt;ObjectConditionList&gt;
    &lt;ObjectCondition IsTemplate="false"&gt;
        &lt;Object&gt;product&lt;/Object&gt;
        &lt;Field&gt;name&lt;/Field&gt;
        &lt;Comparer&gt;=&lt;/Comparer&gt;
        &lt;Value&gt;"Microsoft Teams"&lt;/Value&gt;
        &lt;Operand&gt;string&lt;/Operand&gt;
        &lt;/ObjectCondition&gt;
&lt;/ObjectConditionList&gt;
&lt;ObjectsConditionsExpression&gt;1&lt;/ObjectsConditionsExpression&gt;
&lt;/Investigation&gt;
        </InvestigationRaw>
  </Target>
  <ScriptInfo RunAs="interactiveUser" Timeout="120">
      <WindowsScript Hash="56b4d380788c6b9a41e12caf6317b427c98e4410d31ef04d52f959c4a19ad189"></WindowsScript>
        <Inputs>
    </Inputs>
    <Outputs>
            <Output UID="747cc4a3-b082-d57d-b1c5-f214d8ba4fc7" Name="AuthenticationErrors" Type="uint32" Label="Authentication Errors" Format="ps_boolean" Object="source" UsedBy="WindowsScript">
